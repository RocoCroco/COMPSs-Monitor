        "rawQuery": true,
        "rawSql": "SELECT\n  e.ts AS \"time\",\n  concat(e.thread_type,' #',e.thread_id) AS \"metric\",\n  NULLIF(e.event_code, 0)::double precision AS \"value\"   -- 0 → NULL (no se pinta)\nFROM events e\nWHERE $__timeFilter(e.ts)\n  AND e.agent_id = '127.0.0.1'\n  AND e.thread_type IN ('AP','TD')\n  AND e.event_type = 8001002   -- RUNTIME\nORDER BY e.ts;\n",
